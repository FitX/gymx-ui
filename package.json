{
  "name": "@fitx/gymx-ui",
  "version": "0.31.1",
  "files": [
    "dist",
    "src"
  ],
  "typings": "./dist/types/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*.css": "./dist/styles/*.css",
    "./index.css": "./dist/index.css",
    "./icons/*.svg": "./src/assets/icons/*.svg"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run test:unit -- run --coverage && npm run build:storybook",
    "build:lib": "rm -rf ./dist && vite build && vue-tsc --emitDeclarationOnly --project tsconfig.build-lib.json && tsc-alias -p tsconfig.build-lib.json",
    "build:storybook": "storybook build && npm run temp-fix-copy-storybook",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "temp-fix-copy-storybook": "cp ./coverage/index.html ./storybook-static/coverage/index.html"
  },
  "dependencies": {
    "@volar/language-server": "2.4.10",
    "@vue/language-server": "2.1.10",
    "@vueuse/core": "^11.2.0",
    "colorjs.io": "^0.5.2",
    "react": "^18.3.1",
    "unique-names-generator": "^4.7.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@playwright/test": "^1.49.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-a11y": "^8.4.4",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/addon-onboarding": "^8.4.4",
    "@storybook/addon-themes": "^8.4.4",
    "@storybook/blocks": "^8.4.4",
    "@storybook/test": "^8.4.4",
    "@storybook/vue3": "^8.4.4",
    "@storybook/vue3-vite": "^8.4.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/coverage-v8": "^2.1.5",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.6.0",
    "eslint": "^8.57.1",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-vue": "^9.31.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "storybook": "^8.4.4",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FitX/gymx-ui.git"
  },
  "homepage": "https://gymx-ui-dev.fitx.de"
}
